<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Sauce Demo Webpage">
    <parameter name="useCloudEnv" value="false"></parameter>
    <parameter name="envName" value="browserstack"></parameter>
    <parameter name="os" value="windows"></parameter>
    <parameter name="osVersion" value="11"></parameter>
    <parameter name="browserName" value="chrome"></parameter>
    <parameter name="browserVersion" value="100"></parameter>
    <parameter name="url" value="https://www.saucedemo.com/"></parameter>
    <test name="Sauce Demo Test Cases">
        <classes>
            <class name="test.saucedemotests.LoginPageTest"></class>
            <class name="test.saucedemotests.PurchaseTest"></class>
            <class name="test.saucedemotests.HomePageTest"> </class>
            <class name="test.saucedemotests.YourCartPageTest"></class>
            <class name="test.saucedemotests.YourInfoPageTest"></class>

        </classes>
    </test>
</suite>
